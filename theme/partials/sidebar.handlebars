<div id="sidebar">
    <div id="sidebar_list">
        <div class="versions">
            <h3>版本</h3>
        </div>
        <ul>
            {{#modules}}
                {{#if submodules}}
                    <li class="nav-item">
                        <span class="module">{{displayName}}</span>                 
                            {{#submodules}}                        
                                <div class="nav-group">
                                    <div class="sub-moudle">
                                        {{displayName}}
                                    </div>
                                    <ul class="pure-menu-list sub-nav" style="height: auto;">
                                        {{#classes}}
                                            <li class="nav-item">
                                                <a href="{{../../../../projectRoot}}classes/{{name}}.html" class="">{{name}}</a>
                                            </li>
                                        {{/classes}}
                                    </ul>
                                </div>
                            {{/submodules}}
                    </li>    
                {{else}}
                    <li class="nav-item">
                         <span class="module">{{displayName}}</span>
                         <ul class="pure-menu-list sub-nav" style="height: auto;">
                            {{#classes}}
                                <li class="nav-item">
                                    <a href="{{../../../projectRoot}}classes/{{name}}.html" class="">{{name}}</a>
                                </li>
                            {{/classes}}
                        </ul>
                    </li>    
                {{/if}}
            {{/modules}}
        </ul>
    </div>
